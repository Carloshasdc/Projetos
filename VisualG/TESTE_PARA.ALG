Algoritmo "semnome"

Var
      N, i, x, soma : inteiro

Inicio
          escreva(" digite quantos numeros serão digitados: ")
          leia(n)
          
          soma <- 0
          
          para i de 1 ate N faca
          
          escreva(" digite um numero:")
          leia(x)
          soma <- soma + x
          fimpara
          
          escreval(" SOMA =", soma)

Fimalgoritmo