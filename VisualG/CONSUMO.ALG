Algoritmo "consumo"

Var


       gasto, consumo: real
       distancia : inteiro
   

Inicio

        escreva(" Distancia percorrida: ")
        leia(distancia)
        escreva(" Combustível gasto:")
        leia(gasto)

        
        consumo <- distancia / gasto

        escreval(" Consumo medio ", consumo:8:4)


Fimalgoritmo